#include <stdio.h>

int main() {
    int N;
    int min_value = 1000000;
    int max_value = -1000000;

    scanf("%d", &N);

    for (int i = 0; i < N; i++) {
        int num;
        scanf("%d", &num);

        if (num < min_value)
            min_value = num;
        if (num > max_value)
            max_value = num;
    }

    printf("%d %d\n", min_value, max_value);

    return 0;
}
